<?xml version="1.0" ?>
<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
  "https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
  <suppress message="Missing a Javadoc comment" />
  <suppress message="First sentence of Javadoc is missing an ending period." />
  <!-- Suppressing AbbreviationAsWordInName for common abbreviations like JSON, JWK, URL -->
  <suppress checks="AbbreviationAsWordInName" />
  <!-- Suppressing LocalVariableName for cryptographic variable names like rBytes, sBytes -->
  <suppress checks="LocalVariableName" files="ECUtils\.java" />
  <!-- Suppressing JavadocParagraph for <p> tag formatting -->
  <suppress checks="JavadocParagraph" />
  <!-- Suppressing LineLength for complex generic type declaration -->
  <suppress checks="LineLength" files="ServiceExchange\.java" lines="35" />
  <!-- Suppressing Indentation for switch expression arrow formatting -->
  <suppress checks="Indentation" files="SessionServiceHandler\.java" lines="59" />
</suppressions>
